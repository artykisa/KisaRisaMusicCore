@model KisaRisaMusicCore.Models.Artist
@{
    Layout = "_Layout";
}
@{
    ViewBag.Title = "Track add";
}
<form asp-action="create" asp-controller="Artist">
    <div class="form-group">
        <label asp-for="Nickname" class="control-label">Name</label>
        <input type="text" asp-for="Nickname" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Send" class="btn btn-default" />
    </div>
</form>