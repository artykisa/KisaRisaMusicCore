@model KisaMusic.Domain.Models.Track
@{
    Layout = "_Layout";
}
@{
    ViewBag.Title = "Track add";
}
<form asp-action="create" asp-controller="Track">
    <div class="form-group">
        <label asp-for="Title" class="control-label">Name</label>
        <input type="text" asp-for="Title" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="FileKisaId" class="control-label"></label>
            <select asp-for="FileKisaId" class="form-control" asp-items="ViewBag.FileKisa">
                <option value="">-- Select file --</option>
            </select>
            <span asp-validation-for="FileKisaId" class="text-danger" />
    </div>
    <div class="form-group">
            <label asp-for="ArtistId" class="control-label"></label>
                <select asp-for="ArtistId" class="form-control" asp-items="ViewBag.Artist">
                    <option value="">-- Select file --</option>
                </select>
                <span asp-validation-for="ArtistId" class="text-danger" />
        </div>
        <div class="form-group">
                <label asp-for="AlbumId" class="control-label"></label>
                    <select asp-for="AlbumId" class="form-control" asp-items="ViewBag.Album">
                        <option value="">-- Select file --</option>
                    </select>
                    <span asp-validation-for="AlbumId" class="text-danger" />
            </div>
    <div class="form-group">
        <input type="submit" value="Send" class="btn btn-default" />
    </div>
</form>