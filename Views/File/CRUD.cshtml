@using KisaRisaMusicCore.Models
@model IEnumerable<FileKisa>
@{
    Layout = "_Layout";
}
@{
    ViewData["Title"] = "Home Page";
}
<a asp-action="Create">Add file</a>
<table class="table">
    @foreach (FileKisa p in Model)
    {
        <tr><td>@p.Name</td><td>@p.Path</td><td>@p.Id</td><td>
            <a asp-action="Details" asp-controller="File" asp-route-id="@p.Id">Delatils</a> |
            <a asp-action="Edit" asp-controller="File" asp-route-id="@p.Id">Edit</a> |
            <a asp-action="Delete" asp-controller="File" asp-route-id="@p.Id">Delete</a>
            </td></tr>
    }
</table>